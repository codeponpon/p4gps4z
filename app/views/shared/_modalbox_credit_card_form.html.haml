.row
  #myModal.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "myModalLabel", :role => "dialog", :tabindex => "-1"}
    .modal-dialog
      .modal-content.envelope
        .modal-header
          %button.close{"aria-hidden" => "true", "data-dismiss" => "modal", :type => "button"} Ã—
          %h4#myModalLabel.modal-title= t('text.package.payment_info')
        .modal-body
          = form_tag store_buy_package_path('__PACKAGE_ID__'), :method => 'POST', :id => 'checkout-form', :class => 'smart-form' do
            %fieldset
              %section
                .inline-group
                  %label.radio
                    = radio_button_tag "type", "Visa", true
                    %i>
                    Visa
                  %label.radio
                    = radio_button_tag "type", "mastercard", false
                    %i>
                    MasterCard
                  %label.radio
                    = radio_button_tag "type", "discover", false
                    %i>
                    Discover
              .row
                %section.col.col-6
                  %label.input
                    %i.icon-prepend.fa.fa-user
                    = text_field_tag 'first_name', nil, :placeholder => "First name on card", id: 'first_name'
                %section.col.col-6
                  %label.input
                    %i.icon-prepend.fa.fa-user
                    = text_field_tag 'last_name', nil, :placeholder => "Last name on card", id: 'last_name'
              .row
                %section.col.col-10
                  %label.input
                    = text_field_tag 'number', nil, :placeholder => "1234 5678 9012 3456", id: 'card_number'
                %section.col.col-2
                  %label.input
                    = text_field_tag 'cvv2', nil, :placeholder => "789", id: 'cvv2'
              .row
                %label.label.col.col-4 Expiration date
                %section.col.col-5
                  %label.select
                    %select{:name => "expire_month"}
                      %option{:disabled => "", :selected => "", :value => "0"} Month
                      %option{:value => "1"} January
                      %option{:value => "1"} February
                      %option{:value => "3"} March
                      %option{:value => "4"} April
                      %option{:value => "5"} May
                      %option{:value => "6"} June
                      %option{:value => "7"} July
                      %option{:value => "8"} August
                      %option{:value => "9"} September
                      %option{:value => "10"} October
                      %option{:value => "11"} November
                      %option{:value => "12"} December
                    %i
                %section.col.col-3
                  %label.input
                    = text_field_tag 'expire_year', nil, :placeholder => "2022"
              %footer
                %button.btn.btn-primary{:type => "button"}=t('form.submit')
                %button.btn.btn-default{"data-dismiss" => "modal", :type => "button"}=t('form.cancel')
