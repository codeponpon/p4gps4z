.row
  .large-3.columns
    %h1
      %a{ href: "/" }
        PAGPOS
  .large-9.columns
    %ul.right.button-group
      %li
        - unless current_user.nil?
          = link_to "Tracking", new_tracking_path
          = link_to "Profile", user_profile_path
          = link_to "Signout", destroy_user_session_path, method: :delete
        - else
          = link_to "Twitter", user_omniauth_authorize_path(:twitter)
          = link_to "Google", user_omniauth_authorize_path(:google_oauth2)
          = link_to "Facebook", user_omniauth_authorize_path(:facebook)
          = link_to "Signin", new_user_session_path
/ End Header and Nav